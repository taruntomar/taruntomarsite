@{
    var db = WebMatrix.Data.Database.Open("myconn");
    var selectQueryString = "SELECT * FROM dbo.Login_Table";
}
<html>
<body>
    <h1>Small Bakery Products</h1>
    <table>
        <tr>
            <th>Id</th>
            <th>Name</th>
        </tr>
        @foreach (var row in db.Query(selectQueryString))
        {
            <tr>
                <td>@row.Emp_Id</td>
                <td>@row.Emp_Name</td>
            </tr>
        }
    </table>
</body>
</html>